    "def CleanText(text):\n",
    "    text = text.lower()\n",
    "    text = re.sub(r\"[A-Za-z\\.]*[0-9]+[A-Za-z%¬∞\\.]*\", \"\", text)\n",
    "    text = re.sub(r\"[\\,\\!\\?\\%\\(\\)\\/\\\"\\&\\+\\#\\$\\¬£\\%\\:\\@\\-]\", \"\", text)\n",
    "    text = re.sub(r\"[\\√©\\√®\\√™]\", \"e\", text)\n",
    "    text = re.sub(r\"[\\√π]\", \"u\", text)\n",
    "    text = re.sub(r\"[\\√†]\", \"a\", text)\n",
    "    text = re.sub(r\"[\\√Æ\\√Ø]\", \"i\", text)\n",
    "    text = remove_emojis(text)\n",
    "    text = re.sub(r\"( )+\", \" \", text)\n",
    "    text = re.sub(r\"( )*$\", \"\", text)\n",
    "    \n",
    "    return text"
   ]
